<Application x:Class="Fessad_TL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Fessad_TL"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         <ResourceDictionary>
             <ResourceDictionary.MergedDictionaries>
                 <!-- Order is important-->
                 <ResourceDictionary Source="Dictionries/Colors.xaml"/>
                 <ResourceDictionary Source="Dictionries/Fonts.xaml"/>
                 <ResourceDictionary Source="Dictionries/Buttons.xaml"/>
                 <ResourceDictionary Source="Dictionries/Texts.xaml"/>
                 <ResourceDictionary Source="Dictionries/Windows.xaml"/>
             </ResourceDictionary.MergedDictionaries>
         </ResourceDictionary>
    </Application.Resources>
</Application>
